          }
        },
        "fcf04baa-3b08-4104-9680-e8a8fa9e4f63": {
          "type": "slide",
          "settings": {
            "image": "shopify:\/\/shop_images\/Artboard_1_f67e09f0-dfb2-4f0a-851b-b77f280e5227.png",
            "heading": "",
            "heading_size": "h2",
            "subheading": "\"Combining simplicity with a rugged design, the beauty of the shotgun is the saddle's adjustability. Aboard the shotgun, the tiny passengers can see ahead and begin to learn how to interact with the trail.\"",
            "smallimage": "shopify:\/\/shop_images\/Artboard_3-100.jpg",
            "smallimage_width": 250,
            "button_label": "",
            "link": "",
            "button_toggle": false,
            "box_align": "middle-center",
            "show_text_box": false,
            "text_alignment": "center",
            "image_overlay_opacity": 30,
            "color_scheme": "",
