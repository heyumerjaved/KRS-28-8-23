    "f8bc5375-bc65-4712-86f1-5fd751d0ed64": {
      "type": "slideshow",
      "blocks": {
        "template--16039832944790__f8bc5375-bc65-4712-86f1-5fd751d0ed64-1688424232a10b3944-0": {
          "type": "slide",
          "settings": {
            "image": "shopify:\/\/shop_images\/Artboard_1_f67e09f0-dfb2-4f0a-851b-b77f280e5227.png",
            "heading": "",
            "heading_size": "h2",
            "subheading": "\"The kids ride shotgun seat is the best kids seat for taking your child mountain biking, and is a great way to bond with your child and introduce them to a sport you love\"",
            "smallimage": "shopify:\/\/shop_images\/image_10_2_copy.jpg",
            "smallimage_width": 200,
            "button_label": "",
            "link": "",
            "button_toggle": false,
            "box_align": "middle-center",
