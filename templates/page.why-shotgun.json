    "e5d6bbbd-4f19-4398-a81f-8b7211bcf5bb": {
      "type": "multicolumn-brand-identity",
      "blocks": {
        "template--16066259779734__e5d6bbbd-4f19-4398-a81f-8b7211bcf5bb-column-1": {
          "type": "column",
          "settings": {
            "title": "<strong>Front Mounted Seat<\/strong>",
            "text": "<p>✅ Active participation, learning to ride<\/p><p>✅ Centered weight distribution on bike<\/p><p>✅ Suitable for full suspension bikes<\/p><p>✅ Visibility of what's ahead<\/p><p>✅ Ability to interact \/ talk with child<\/p>",
            "link_label": "",
            "link": ""
          }
        },
        "template--16066259779734__e5d6bbbd-4f19-4398-a81f-8b7211bcf5bb-column-2": {
          "type": "column",
          "settings": {
            "title": "<strong>Rear Mounted Seat<\/strong>",
            "text": "<p>⛔️ Passive participation, sitting prone<\/p><p>⛔️ Uneven weight distribution on bike<\/p><p>⛔️ Suitable for hard tail bikes only <\/p><p>⛔️ Looking at parents back<\/p><p>⛔️ Limited ability to interact \/ talk with child<\/p>",
            "link_label": "",
            "link": ""
          }
        }
      },
