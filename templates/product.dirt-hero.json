    "3de35d8a-2b33-4a2a-99e1-32d2b23fdc8a": {
      "type": "collapsible-content",
      "blocks": {
        "template--16038410911894__3de35d8a-2b33-4a2a-99e1-32d2b23fdc8a-16871435106c3053cf-0": {
          "type": "collapsible_row",
          "settings": {
            "heading": "Sizing",
            "icon": "none",
            "row_content": "<p><strong>12\"<\/strong><\/p><p>- Age: 2 - 3 years <\/p><p>- Minimum suggested inseam: 337mm \/ 13.3\"<\/p><p>- Minimum saddle height: 317mm \/ 12.5\"<\/p><p><\/p><p><strong>14\" <\/strong><\/p><p>- Age: 3 - 5 years <\/p><p>- Minimum suggested inseam: 367mm \/ 14.4\"<\/p><p>Minimum saddle height: 346mm \/ 13.6\"<\/p>",
            "page": "",
            "video_link": "",
            "htmltable": ""
          }
        },
        "template--16038410911894__3de35d8a-2b33-4a2a-99e1-32d2b23fdc8a-16871435106c3053cf-1": {
          "type": "collapsible_row",
          "settings": {
            "heading": "Specs",
            "icon": "none",
            "row_content": "<p><strong>Bike weight <\/strong><\/p><ul><li>12\": 4kg <\/li><li>12\" with brake: 4.4kg<\/li><li>14\": 4.2kg <\/li><li>14\" with brake: 4.6kg<\/li><\/ul><p><\/p><p><strong>Wheels <\/strong><\/p><ul><li>Spokes: 2mm stainless steel <\/li><li>Lacing: 16x radial lacing <\/li><li>Front hub: KRS custom through axle 100mm<\/li><li>Rear hub: KRS custom through axle with disc brake mount 100mm<\/li><\/ul><p><\/p><p><strong>Tyres<\/strong><\/p><ul><li>Vee Tire Crown Gem 2.25\" 12\"\/14\" <\/li><\/ul><p><\/p><p><strong>Cockpit <\/strong><\/p><ul><li>Handlebar: KRS custom. 25.4mm clamp, 19mm handles, Up sweep 6 degrees, backsweep 8 degrees, length 450mm, rise 20mm<\/li><li>Grips: KRS custom 19mm <\/li><li>Stem: Forged 6061 T6 Aluminium, Rise: 0, Reach 30mm, Clamp: 25.4mm<\/li><li>Bearings: NECO H125<\/li><li>Headset: Semi integrated headset (sealed cartridge bearings)<\/li><\/ul><p><\/p><p><strong>Frame \/ Other<\/strong><\/p><ul><li>Frame material: 6061-T6 aluminium <\/li><li>Head tube: Standard 1 1\/8\" semi integrated headset compatible<\/li><li>Seat post: Aluminuim 6061- T6, 27.2mm<\/li><li>Fork: 6061-T6 aluminium<\/li><li>Bearings: NECO H125<\/li><li>Colour: Artic storm white, matte <\/li><\/ul><p><\/p><p><strong>Brake <\/strong><\/p><ul><li>Brake model: Magura MT4 post mount<\/li><li>Master cylinder material: Carbotecture<\/li><li>Blade Lever: Magura HC1, aluminium<\/li><li>Caliper: Magura MT4 - Aluminium (OE version)<\/li><li>Rotor: Storm HC 140<\/li><li>Hose length: 750mm<\/li><li>Fluid typeMineral oil<\/li><li>Pad: Organic<\/li><\/ul><p><br\/><\/p>",
            "page": "",
            "video_link": "",
            "htmltable": ""
          }
        },
        "template--16038410911894__3de35d8a-2b33-4a2a-99e1-32d2b23fdc8a-16871435106c3053cf-2": {
          "type": "collapsible_row",
          "settings": {
            "heading": "Geometry",
            "icon": "none",
            "row_content": "",
            "page": "",
            "video_link": "",
            "image_cr": "shopify:\/\/shop_images\/Screenshot_2023-09-26_at_12.02.10_PM.png",
            "htmltable": "<table>\n  <tbody>\n<tr>\n    <th> <\/th>\n    <th>12\"<\/th>\n    <th>14\"<\/th>\n  <\/tr>\n  <tr>\n    <td>A. Seated reach<\/td>\n    <td>301-340mm<\/td>\n    <td>301-340mm<\/td>\n  <\/tr>\n  <tr>\n    <td>B. Stack<\/td>\n    <td>312mm<\/td>\n    <td>312mm<\/td>\n  <\/tr>\n  <tr>\n    <td>C. Effective TT length<\/td>\n    <td>348mm<\/td>\n    <td>348mm<\/td>\n  <\/tr>\n  <tr>\n <td>D. Seat tube length<\/td>\n    <td>155mm<\/td>\n    <td>155mm<\/td>\n  <\/tr>\n  <tr>\n <td>E. Seat tube angle<\/td>\n    <td>65°<\/td>\n    <td>65°<\/td>\n  <\/tr>\n  <tr>\n  <td>F. Head tube length<\/td>\n    <td>83mm<\/td>\n    <td>83mm<\/td>\n  <\/tr>\n  <tr>\n  <td>G. Head tube angle<\/td>\n    <td>66°<\/td>\n<td>66°<\/td>\n  <\/tr>\n  <tr>\n <td>H. Chainstay length<\/td>\n    <td>267mm<\/td>\n<td>281mm<\/td>\n  <\/tr>\n  <tr>\n <td>I. Wheel base<\/td>\n    <td>612mm<\/td>\n<td>646mm<\/td>\n  <\/tr>\n  <tr>\n <td>J. Bike height<\/td>\n    <td>105mm<\/td>\n<td>140mm<\/td>\n  <\/tr>\n  <tr>\n <td>K. Seat height<\/td>\n    <td>317-402mm<\/td>\n<td>352-437mm<\/td>\n  <\/tr>\n  <tr>\n <td>L. Fork offset<\/td>\n    <td>29mm<\/td>\n<td>41.5mm<\/td>\n  <\/tr>\n  <tr>\n<\/tr>\n<\/tbody>\n<\/table>\n\n<style>\n  table {\n    border-collapse: collapse;\n    width: 95%;\n    border-radius: 6px;\n    margin: auto;\n  }\n\n  th, td {\n    border: 6px #212121;\n    padding: 8px;\n    text-align: center;\n  }\n\n  th {\n    font-weight: 900;\n    font-size: 15px;\n    color: #606060;\n  }\n\n  td {\n    font-weight: 900;\n    font-size: 10px;\n    color: #212121;\n  }\n\n  td:first-child {\n    text-align: left;\n  }\n<\/style>"
